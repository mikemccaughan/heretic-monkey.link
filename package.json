    "cookie": "^1.0.2",
    "cross-spawn": "^7.0.6",
    "ejs": "^3.1.10",
    "esbuild": "^0.25.0",
    "express": "5",
    "http-proxy-middleware": "^3.0.3",
    "jsonpath-plus": "^10.3.0",
    "loader-utils": "^3.3.1",
    "micromatch": "^4.0.8",
    "nanoid": "^5.1.2",
    "path-to-regexp": "^8.2.0",
    "rollup": "^4.34.8",
    "send": "^1.1.0",
