{
  "name": "heretic-monkey.link",
  "version": "0.1.0",
  "description": "",
  "main": "server.cjs",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "workspaces": [
    "hm-walls",
    "hm-walls-api",
    "material-test"
  ],
  "scripts": {
    "start": "yarn node server.cjs",
    "build:yarn": "yarn set version stable",
    "build:directories": "rm -rf publish && mkdir -p publish/material-test && mkdir -p publish/mynsweepr-angular && mkdir -p publish/mynsweepr-hooks && mkdir -p publish/mynsweepr-react && mkdir -p publish/mynsweepr-react19",
    "build:nobuild": "yarn install && cp *.*js publish/ && cp *.html publish/ && cp *.css publish/ && cp -r hm-grid publish/hm-grid && cp -r design publish/design",
    "build:workspaces": "yarn workspaces focus --all && yarn workspaces foreach -v --all --exclude heretic-monkey.link run build && ./build-mynsweeprs.sh",
    "build:predeploy": "cp -r material-test/dist publish/material-test",
    "build": "yarn run build:yarn && yarn run build:directories && yarn run build:nobuild && yarn run build:workspaces && yarn run build:predeploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikemccaughan/heretic-monkey.link.git"
  },
