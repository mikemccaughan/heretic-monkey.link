{
  "name": "hm-walls-api",
  "version": "1.0.0",
  "description": "API for providing Wallpaper images and associated metadata to the hm-walls app",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run prettier:fix && npm run tslint:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.4.1",
    "@loopback/context": "^3.9.4",
    "@loopback/core": "^2.9.3",
    "@loopback/openapi-v3": "^3.4.7",
    "@loopback/repository": "^3.4.0",
    "@loopback/rest": "^5.2.1",
    "@loopback/rest-explorer": "^2.2.8",
    "@loopback/service-proxy": "^2.3.6",
    "azure-storage": "^2.10.3",
    "loopback-connector-rest": "^3.7.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.0",
    "@loopback/testlab": "^3.2.2",
    "@loopback/tslint-config": "^2.1.0",
    "@types/node": "^14.0.27",
    "mocha": "^8.1.1",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  }
}
