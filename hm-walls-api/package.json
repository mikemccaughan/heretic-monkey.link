{
  "name": "hm-walls-api",
  "version": "1.0.0",
  "description": "API for providing Wallpaper images and associated metadata to the hm-walls app",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run prettier:fix && npm run tslint:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^4.0.0",
    "@loopback/context": "^4.0.0",
    "@loopback/core": "^3.0.0",
    "@loopback/openapi-v3": "^7.0.0",
    "@loopback/repository": "^4.0.0",
    "@loopback/rest": "^11.0.0",
    "@loopback/rest-explorer": "^4.0.0",
    "@loopback/service-proxy": "^4.0.0",
    "azure-storage": "^2.10.5",
    "loopback-connector-rest": "^4.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^8.0.0",
    "@loopback/testlab": "^4.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.32.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.2"
  }
}
